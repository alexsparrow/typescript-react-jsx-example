{
    "compilerOptions": {
        "outDir": "dist",
        "target": "es5",
        "module": "commonjs",
        "jsx": "react",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "filesGlob": [
        "./**/*.ts",
        "./**/*.tsx",
        "!./node_modules/**/*"
    ],
    "files": [
        "typings/react-bootstrap/react-bootstrap.d.ts",
        "typings/react-redux/react-redux.d.ts",
        "typings/react/react-dom.d.ts",
        "typings/react/react.d.ts",
        "typings/redux/redux.d.ts",
        "typings/tsd.d.ts",
        "components/App.tsx",
        "components/hello.tsx",
        "components/Nav.tsx",
        "index.tsx"
    ],
    "exclude": []
}
